<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seguranca.mas.planus.model">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/iconeplanus"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="seguranca.mas.planus.planusmasseguranca.MenuPrincipalLateral"
            android:label="@string/title_activity_menu_principal_lateral"
            android:theme="@style/AppTheme" />
        <activity android:name="seguranca.mas.planus.Activity.ChamadaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="seguranca.mas.planus.Activity.TelaInicialActivity" />
        <activity android:name="seguranca.mas.planus.Activity.MensagemActivity" />
        <activity android:name="seguranca.mas.planus.Activity.ValidadorActivity" />
        <activity android:name="seguranca.mas.planus.Activity.MenuCentralActivity" />
        <activity android:name="seguranca.mas.planus.Activity.PainelDeMensagensActivity" />
        <activity android:name="seguranca.mas.planus.Activity.CadastroDeUsuariosActivity" />
        <activity android:name="seguranca.mas.planus.Activity.InformarRoubo" />
        <activity android:name="seguranca.mas.planus.Activity.MainActivity" />
        <activity android:name="seguranca.mas.planus.Activity.ActivityConversas" />
        <activity android:name="seguranca.mas.planus.planusmasseguranca.UploadDeFotosActivity" />

        <service
            android:name="seguranca.mas.planus.services.MyServiceStartPlanus"
            android:enabled="true"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="seguranca.mas.planus.Activity.CadastroDeOcorrencias"
            android:label="@string/title_activity_cadastro_de_ocorrencias"
            android:theme="@style/AppTheme" />
        <activity
            android:name="seguranca.mas.planus.planusmasseguranca.ActivityCercaEletronica"
            android:label="@string/title_activity_cerca_eletronica"
            android:theme="@style/AppTheme" />
        <activity android:name="seguranca.mas.planus.planusmasseguranca.TrocaDeMensagemActivity" />
        <activity android:name="seguranca.mas.planus.planusmasseguranca.ActivityAvisoSemInternet" />
        <activity android:name="seguranca.mas.planus.planusmasseguranca.ActivityMenuComerciario" />
        <activity android:name="seguranca.mas.planus.planusmasseguranca.ActivityTelefoneUteis"></activity>
    </application>

</manifest>